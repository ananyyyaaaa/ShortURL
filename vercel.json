{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/views/$1"
      }
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "/views/new.ejs"
      }
    ]
  }
  